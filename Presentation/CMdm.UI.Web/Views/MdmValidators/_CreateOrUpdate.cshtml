@model CMdm.UI.Web.Models.DqValidator.DqValidatorModel
@Html.ValidationSummary(true)
@Html.HiddenFor(model => model.REGEX_ID)

<div class="content">
    <div class="form-horizontal">
        <div class="panel-group">
            <div class="panel panel-default">
                <div class="panel-body">
                    <div>
                        <div class="form-group">
                            <div class="col-md-3">
                                @Html.LabelFor(model => model.REGEX_NAME)
                            </div>
                            <div class="col-md-9">
                                @Html.CMdmEditorFor(model => model.REGEX_NAME, required: true)
                                @Html.ValidationMessageFor(model => model.REGEX_NAME)
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.REGEX_DESC)
                        </div>
                        <div class="col-md-9">
                            @Html.CMdmEditorFor(model => model.REGEX_DESC)
                            @Html.ValidationMessageFor(model => model.REGEX_DESC)
                        </div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-3">
                            @Html.LabelFor(model => model.REGEX_STRING)
                        </div>
                        <div class="col-md-9">
                            @Html.CMdmEditorFor(model => model.REGEX_STRING)
                            @Html.ValidationMessageFor(model => model.REGEX_DESC)
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


