                                    @Html.ValidationMessageFor(model => model.STATE_OF_ORIGIN, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label> Mother Maiden Name</label>
                                    @Html.EditorFor(model => model.MOTHER_MAIDEN_NAME, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.MOTHER_MAIDEN_NAME, "", new { @class = "text-danger" })


                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label>Disability</label>

                                    @Html.EditorFor(model => model.DISABILITY, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.DISABILITY, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <label>Complexion</label>
                                    @Html.EditorFor(model => model.COMPLEXION, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.COMPLEXION, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label> Number of Children</label>
                                    @Html.EditorFor(model => model.NUMBER_OF_CHILDREN, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.NUMBER_OF_CHILDREN, "", new { @class = "text-danger" })


                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label>Religion</label>
                                    @Html.EditorFor(model => model.RELIGION, new { htmlAttributes = new { @class = "form-control" } })
