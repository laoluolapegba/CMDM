@model CMdm.Entities.Domain.Customer.cDMA_INDIVIDUAL_IDENTIFICATION

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}


<style>
    .nav-tabs-custom > .tab-content {
        color: black;
    }
   label
    {
       display:block;       

    }

    

   
</style> 
<div class="col-md-6">
    <!-- Custom Tabs (Pulled to the right) -->
    <div class="nav-tabs-custom" style=" width:850px;">
        <ul class="nav nav-tabs pull-right">
            <li class="active"><a href="#tab_1-1" data-toggle="tab">Edit Profile</a></li>
            <li><a href="#tab_2-2" data-toggle="tab">Change History</a></li>
          <!--<li><a href="#tab_3-2" data-toggle="tab">Other Details</a></li>-->
             
            <li class="pull-left header"><i class="fa fa-th"></i>@ViewBag.record.SURNAME @ViewBag.record.FIRST_NAME @ViewBag.record.OTHER_NAME</ li >
        </ul>
        <div class="tab-content" >
            <div class="tab-pane active"  id="tab_1-1">
                
                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-horizontal">
                        <h4>CUSTOMER BIO DATA</h4>
                        <hr />
                        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                        @Html.HiddenFor(model => model.CUSTOMER_NO)
                        <div class="row">
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <label> Title</label>

                                    @Html.EditorFor(model => model.TITLE, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.TITLE, "", new { @class = "text-danger " })

                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label> Surname</label>

                                    @Html.EditorFor(model => model.SURNAME, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.SURNAME, "", new { @class = "text-danger" })

                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label>First Name</label>

                                    @Html.EditorFor(model => model.FIRST_NAME, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.FIRST_NAME, "", new { @class = "text-danger" })

                                </div>
                            </div>
                        </div>


                        <div class="row">
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <label> Other Name</label>

                                    @Html.EditorFor(model => model.OTHER_NAME, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.OTHER_NAME, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label> Nickname</label>

                                    @Html.EditorFor(model => model.NICKNAME_ALIAS, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.NICKNAME_ALIAS, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label>Date of Birth</label>

                                    @Html.EditorFor(model => model.DATE_OF_BIRTH, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.DATE_OF_BIRTH, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>



                        <div class="row">
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <label> Place of Birth</label>
                                    @Html.EditorFor(model => model.PLACE_OF_BIRTH, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.PLACE_OF_BIRTH, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label> Country of Birth</label>

                                    @Html.EditorFor(model => model.COUNTRY_OF_BIRTH, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.COUNTRY_OF_BIRTH, "", new { @class = "text-danger" })
                                </div>
                            </div>
                            <div class="col-lg-4">
                                <div class="input-group">
                                    <label>Sex</label>

                                    @Html.EditorFor(model => model.SEX, new { htmlAttributes = new { @class = "form-control" } })
                                    @Html.ValidationMessageFor(model => model.SEX, "", new { @class = "text-danger" })
                                </div>
                            </div>
                        </div>



                        <div class="row">
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <label> Age</label>
                                    @Html.EditorFor(model => model.AGE, new { htmlAttributes = new { @class = "form-control" } })
